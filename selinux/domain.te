#allow domain powervr_device:chr_file rw_file_perms;

## Firmwares
allow ueventd { firmware_ducati }:file r_file_perms;
allow domain domain:{ shm sem msg msgq } *;
ifelse(shipping_build, `true',
  `allow { domain -system_server -system_app -init -installd } system_data_file:file no_w_file_perms;'
,
  `neverallow { domain -system_server -system_app -init -installd -recovery } system_data_file:file no_w_file_perms;'
)
